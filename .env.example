# Enable debug mode (set to true for verbose logging, false for production)
DEBUG=true

# Session timeout in seconds (default: 1800, i.e., 30 minutes)
SESSION_TIMEOUT=1800

# Interval in seconds for cleaning up expired sessions (default: 300, i.e., 5 minutes)
SESSION_CLEANUP_INTERVAL=300

# Max age in seconds for the session cookie (default: 1800, i.e., 30 minutes)
SESSION_COOKIE_MAX_AGE=1800

# Google API key for external integrations (currently not used directly in backend)
GOOGLE_API_KEY=

# Model provider for agents (e.g., Gemini, LMStudio; default: Gemini)
AGENTS_MODEL_PROVIDER=Gemini

# Model ID for the selected agent model (e.g., gemini-2.0-flash)
AGENTS_MODEL_ID=

# Maximum number of resume versions to keep per session (default: 5)
MAX_RESUMES_PER_SESSION=5

# Environment setting (development/production) - affects cookie security and other settings
ENVIRONMENT=development

# Frontend URL for CORS (use * for development, specific domain for production)
FRONTEND_URL=http://localhost:8000 